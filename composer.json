{
  "name": "spinzar/laravel-package-generator",
  "description": "A laravel package generator",
  "license": "MIT",
  "keywords": [
    "laravel",
    "package",
    "generator"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Nassim Nasibullah",
      "email": "spinzar.inc@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0.2",
    "illuminate/container": "^9.0",
    "illuminate/console": "^9.0",
    "illuminate/filesystem": "^9.0",
    "illuminate/support": "^9.0",
    "illuminate/view": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Spinzar\\LaravelPackageGenerator\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Spinzar\\LaravelPackageGenerator\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
